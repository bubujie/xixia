<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/1.6/template-install.dtd">
  <extension version="2.5" type="template" client="site">
    <name>Beyond</name>
    <creationDate>2012年02月02日</creationDate>
    <author>步步街</author>
    <authorEmail>bubujie@gmail.com</authorEmail>
    <authorUrl>http://www.bubujie.net/</authorUrl>
    <copyright>Copyright (C) 2008-2012 BuBuJie.Net All Rights Reserved.</copyright>
    <license>商业用户单域名授权</license>
    <version>1.0.0</version>
    <description>TPL_TUDING_DESCRIPTION</description>
    <files>
      <folder>language</folder>
      <folder>fonts</folder>
      <folder>html</folder>
      <folder>css</folder>
      <folder>img</folder>
      <folder>js</folder>
      <filename>template_thumbnail.png</filename>
      <filename>template_preview.png</filename>
      <filename>templateDetails.xml</filename>
      <filename>component.php</filename>
      <filename>favicon.ico</filename>
      <filename>index.html</filename>
      <filename>index.php</filename>
      <filename>error.php</filename>
    </files>
    <positions>
      <position>debug</position>
      <position>crumbs</position>
      <position>search</position>
      <position>bh1st-side</position>
      <position>bh1st-main</position>
      <position>bh1st-aside</position>
      <position>bh5th-side</position>
      <position>bh5th-main</position>
      <position>bh5th-aside</position>
      <position>bh9th-side</position>
      <position>bh9th-main</position>
      <position>bh9th-aside</position>
      <position>bb1st-side</position>
      <position>bb1st-main</position>
      <position>bb1st-aside</position>
      <position>side-top</position>
      <position>side-mid</position>
      <position>side-btm</position>
      <position>com-top</position>
      <position>com-btm</position>
      <position>aside-top</position>
      <position>aside-mid</position>
      <position>aside-btm</position>
      <position>bb9th-side</position>
      <position>bb9th-main</position>
      <position>bb9th-aside</position>
      <position>bf1st-side</position>
      <position>bf1st-main</position>
      <position>bf1st-aside</position>
      <position>bf5th-side</position>
      <position>bf5th-main</position>
      <position>bf5th-aside</position>
      <position>bf9th-side</position>
      <position>bf9th-main</position>
      <position>bf9th-aside</position>
    </positions>
    <languages folder="language">
      <language tag="en-GB">en-GB/en-GB.tpl_tuding.ini</language>
      <language tag="en-GB">en-GB/en-GB.tpl_tuding.sys.ini</language>
      <language tag="zh-CN">zh-CN/zh-CN.tpl_tuding.ini</language>
      <language tag="zh-CN">zh-CN/zh-CN.tpl_tuding.sys.ini</language>
    </languages>
    <config>
      <fields name="params">
        <fieldset name="advanced">
          <field name="bheadLogo" type="media" hide_default="1" label="TPL_TUDING_FIELD_BHEADLOGO_LABEL" description="TPL_TUDING_FIELD_BHEADLOGO_DESC" />
          <field name="bfootLogo" type="media" hide_default="1" label="TPL_TUDING_FIELD_BFOOTLOGO_LABEL" description="TPL_TUDING_FIELD_BFOOTLOGO_DESC" />
          <field name="siteTitle" type="text" default="" label="TPL_TUDING_FIELD_SITETITLE_LABEL" description="TPL_TUDING_FIELD_SITETITLE_DESC" filter="string" />
          <field name="siteDesc" type="text" default="" label="TPL_TUDING_FIELD_SITEDESC_LABEL" description="TPL_TUDING_FIELD_SITEDESC_DESC" filter="string" />
        </fieldset>
      </fields>
    </config>
  </extension>